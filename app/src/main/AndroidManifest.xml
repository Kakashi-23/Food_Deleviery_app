<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.s.b.s.ideapad.food_delivery_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/foodapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="activity.RegisterHere"
            android:parentActivityName="activity.Login"></activity>
        <activity android:name="activity.FPSecPage" />
        <activity android:name="activity.ForgotPassword" />
        <activity android:name="activity.SplashPayment" />
        <activity
            android:name="activity.CartPage"
            android:parentActivityName="activity.AllRestaurantPage" />
        <activity android:name="activity.ResMenuRdf"
            android:parentActivityName="activity.AllRestaurantPage"/>
        <activity android:name="activity.AllRestaurantPage" />
        <activity android:name="activity.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activity.Login" />
    </application>

</manifest>